<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.geometry.Insets?>

<VBox>
    <children>
        <HBox>
            <padding><Insets top="20" right="20" bottom="10" left="20"/></padding>
            <Label text = "File Name">
                <padding><Insets right="10"/></padding>
            </Label>
            <TextField />
            <Button text = "Browse"/>
        </HBox>
        <HBox>
            <padding><Insets top="10" right="20" bottom="10" left="20"/></padding>
            <Label text = "Target Field">
                <padding><Insets right="10"/></padding>
            </Label>
            <TextField />
        </HBox>
        <HBox>
            <padding><Insets top="10" right="20" bottom="10" left="20"/></padding>
            <Label text = "Target Value">
                <padding><Insets right="10"/></padding>
            </Label>
            <TextField />
        </HBox>
        <HBox>
            <padding><Insets top="10" right="20" bottom="20" left="150"/></padding>
            <Button text = "Cancel"/>
            <Button text = "OK"/>
        </HBox>
    </children>
</VBox>

